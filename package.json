  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass src/client/sass/main.scss src/client/css/style.css -w",
    "compile:sass": "node-sass src/client/sass/main.scss src/client/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/client/css/style.comp.css -o src/client/css/style.prefix.css",
    "compress:css": "node-sass src/client/css/style.prefix.css src/client/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "production": "webpack --config webpack.client.js --mode production",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "start": "node build/bundle.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
